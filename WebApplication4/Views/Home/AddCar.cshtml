<!DOCTYPE html>
@model string
<html>
<head>
    <link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.0/js/bootstrap.min.js"></script>
    <script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
    <title>Login Page</title>
    <style>
        .well {
            padding: 35px;
            padding-left: 30px;
            box-shadow: 0 0 10px #666666;
            margin: 4% auto 0;
            width: 450px;
            background-color: rgba(0,0,0,0.7) !important;
        }

        html, body {
            background-image: url('https://d.newsweek.com/en/full/1549703/porsche-macan-2019-south-korea-auto-show.jpg');
            background-size: cover;
            background-repeat: no-repeat;
            height: 100%;
            background-color: #dedede;
        }

        .input-group-addon {
            background-color: black;
            color: white;
        }

        .excel {
            font-family: Code;
            font-size: xx-large;
            color: white;
        }

        .adj {
            margin-left: 130px;
        }
    </style>

    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">

    <!--Fontawesome CDN-->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.3.1/css/all.css" integrity="sha384-mzrmE5qonljUremFsqc01SB46JvROS7bZs3IO2EmfFsd15uHvIt+Y8vEf7N7fWAU" crossorigin="anonymous">
    <!--Custom styles-->
    <link rel="stylesheet" type="text/css" href="styles.css">
</head>
<body>
    <br />
    <h1 class="display-1 text-center font-weight-bold">Hyper Uber</h1>
    <form action="../../Home/AddCarToDataBase" name="AddCarToDataBase" method="post" onsubmit="return(validate());">
        <div class="container-fluid">
            <div class="row">
                <div class="well center-block">
                    <div class="well-header">
                        <h3 class="text-center excel"> Car Registration </h3>
                        <hr>
                    </div>
                    <div class="row">
                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <div class="form-group">
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="glyphicon glyphicon-user"></i>
                                    </div>
                                    <input type="text" placeholder="Name(Textual Alphabets)" name="Name" class="form-control">

                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12 col-sm-12 col-md-12">
                            <div class="form-group">
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="glyphicon glyphicon-qrcode"></i>
                                    </div>
                                    <input type="text" placeholder="(Number Plate)Lxx-xxx" name="NumberPlate" class="form-control">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <div class="form-group">
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="glyphicon glyphicon-lock"></i>
                                    </div>
                                    <input type="number" placeholder="(Model)e.g:2017" name="Model" class="form-control">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12 col-xs-12 col-sm-12">
                            <div class="form-group">
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="glyphicon glyphicon-phone"></i>
                                    </div>
                                    <input type="text" class="form-control" name="Make" placeholder="(Make)e.g:Honda">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12 col-xs-12 col-sm-12">
                            <div class="form-group">
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="glyphicon glyphicon-envelope"></i>
                                    </div>
                                    <input type="number" class="form-control" name="PetrolInLitres" placeholder="PetrolInLitres.e.g:90">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12 col-xs-12 col-sm-12">
                            <div class="form-group">
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="glyphicon glyphicon-list-alt"></i>
                                    </div>
                                    <textarea class="form-control" name="DrivenInKms" placeholder="DrivenInKms.e.g:90000.9"></textarea>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12 col-xs-12 col-sm-12">
                            <div class="form-group">
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="glyphicon glyphicon-stats"></i>
                                    </div>
                                    <input type="number" name="EngineCC" placeholder="EngineCC.660" class="form-control">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12 col-sm-12 col-md-12">
                            <div class="form-group">
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="glyphicon glyphicon-qrcode"></i>
                                    </div>
                                    <input type="text" placeholder="Cnic(xxxxx-xxxxxxx-x)" name="Cnic" class="form-control">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row widget">
                        <div class="col-md-12 col-xs-12 col-sm-12">
                            <button class="btn btn-primary btn-block"> Register Car </button>
                        </div>
                    </div>
                    <div class="d-flex justify-content-center">
                        <a href="../../Home/RentYourCar">Cancel Register?Go Back</a>
                    </div>

                    @{

                        if (Model == "Same Car Already Exists, Check Credentials")
                        {

                            <div class="adj"><label style="color:red" ;>@Model</label></div>
                        }
                        else
                        {
                            <div class="adj"><label style="color:green" ;>@Model</label></div>
                        }
                    }
                </div>
            </div>
        </div>

    </form>
    <script>
        function validate() {
            if (document.myForm.Name.value == "") {
                alert("Please Enter First Name");
                document.myForm.Name.focus();
                return false;
            }

            if (document.myForm.Cnic.value == "") {
                alert("Please Enter Cnic");
                document.myForm.Cnic.focus();
                return false;
            }

            if (document.myForm.Password.value == "") {
                alert("Please Create your Password");
                document.myForm.Password.focus();
                return false;
            }

            if (document.myForm.PhoneNo.value == "") {
                alert("Please enter Mobile Number");
                document.myForm.PhoneNo.focus();
                return false;
            }

            if (document.myForm.Email.value == "") {
                alert("Please Enter E-Mail ID");
                document.myForm.Email.focus();
                return false;
            }

            if (document.myForm.Address.value == "") {
                alert("Please Enter Address");
                document.myForm.Address.focus();
                return false;
            }

            if (document.myForm.YearsOfExperience.value == "") {
                alert("Please Enter Years Of Experience");
                document.myForm.YearsOfExperience.focus();
                return false;
            }
        }
    </script>
</body>
</html>