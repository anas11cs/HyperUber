
@{
    if (Session["email"] == null)
    {
        Response.Redirect("~/Home/index");
    }
}
<!DOCTYPE html>

@model List<String>


<html>
<head>
    <title>Share Market</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <style>
        ul {
            list-style-type: none;
            margin: 0;
            padding: 0;
            overflow: hidden;
            border: 1px solid #e7e7e7;
            background-color: white;
        }

        li {
            float: left;
        }

            li a {
                display: block;
                color: #666;
                text-align: center;
                padding: 14px 16px;
                text-decoration: none;
            }

                li a:hover:not(.active) {
                    background-color: #ddd;
                }

        .head1 {
            font-size: 50px;
            font-weight: bolder;
            margin-left: 400px;
            font-family: 'Agency FB';
            margin-bottom: 1px;
            color: white;
            /* THE CHANGE TO ADD IS BELOW*/
            text-shadow: 1px 2px 1px 2px #999;
        }

        .head2 {
            font-size: 12px;
            margin-left: 550px;
            margin-bottom: 8px;
            font-weight: bolder;
            color: white
        }

        .head3 {
            font-size: 10px;
            margin-left: 440px;
            margin-bottom: 20px;
            font-weight: bolder;
            color: white
        }

        .body_sec {
            margin-left: 480px;
        }

        .body_sec1 {
            margin-left: 10px;
            margin-bottom: 15px;
        }

        .body_sec2 {
            margin-bottom: 20px;
            margin-left: 10px;
        }

        .body_sec3 {
            margin-left: 485px;
        }

        .foot {
            margin-top: 20px
        }

        .button:active {
            background-color: black;
            box-shadow: 0 5px #272a3c;
            transform: translateY(4px);
        }

        .button:hover {
            background-color: white;
        }

        .button {
            padding: 2.5px 10px;
            font-size: 12px;
            font-family: Bebas;
            text-align: center;
            cursor: pointer;
            outline: none;
            color: black;
            background-color: firebrick;
            border: none;
            border-radius: 10px;
            box-shadow: 0 4px #999;
            margin-left: 535px;
        }

        .jangli {
            display: block;
            color: #666;
            text-align: center;
            padding: 14px 16px;
            text-decoration: none;
        }

        .head4 {
            /*padding:10px 5px 15px 20px;
        top padding is 10px
        right padding is 5px
        bottom padding is 15px
        left padding is 20px*/
            font-size: 75px;
            font-weight: bolder;
            font-family: 'Script MT';
            color: white;
            border-radius: 60px;
            text-shadow: 3px 3px 3px 3px #656565;
            margin-bottom: 15px;
            margin-left: 370px;
        }

            .head4:hover {
                text-shadow: 2.5px 2.5px 2.5px 2.5px #a4fff7;
                color: black;
            }

        .fnt {
            margin-top: 15px;
            color: black;
            font-size: 20px;
            font-weight: bolder;
            font-family: Aaargh;
        }

        .datee {
            margin-left: 490px;
            font-size: 10px;
            margin-bottom: 35px;
            color: white;
        }
    </style>
</head>
<body style="background:black">
    <div><p class="head4">Cars Available For Rent</p></div>
    <div class="w3-container">

        <table class="w3-table w3-pale-blue">
            <thead>
                <tr class="w3-white w3-hover-grey w3-hover-text-white">
                    <th class="fnt">Cars</th>
                </tr>
            </thead>
            @{
                if (Model != null)
                {

                    foreach (var item in Model)
                    {

                        <tr class="w3-hover-text-black w3-hover-white w3-text-black">
                            <td>@item</td>
                        </tr>

                    }
                }
            }

        </table>

    </div>
    <br />
    <a class="w3-text-white" href="../../Home/RentYourCar">Back to Home</a>
    <hr />
</body>
</html>
